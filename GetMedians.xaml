    <ui:InvokeCode ContinueOnError="{x:Null}" Code="Dim MED As Double&#xD;&#xA;&#xD;&#xA;Centralizator = in_TransactionData.Clone 'get table headers&#xD;&#xA;'design table&#xD;&#xA;Centralizator.Columns.Remove(&quot;CUI&quot;)&#xD;&#xA;Centralizator.Columns.Remove(&quot;Subdepartament&quot;)&#xD;&#xA;Centralizator.Columns.Remove(&quot;Departament&quot;)&#xD;&#xA;Centralizator.Columns(&quot;FUNCTIE&quot;).ColumnName = &quot;DEP&quot;&#xD;&#xA;&#xD;&#xA;'create Departament values list&#xD;&#xA;Dim Dep = New List(Of String)&#xD;&#xA;Dep = (From r As DataRow In in_TransactionData.Rows.Cast(Of DataRow)()Select convert.ToString(r(&quot;Departament&quot;))).ToList&#xD;&#xA;Dep = Dep.Distinct.ToList&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;For Each departament As String In Dep&#xD;&#xA;&#x9;Dim MedList As New list(Of String)&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;MedList.Add(departament.ToString)&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;'get medians for departaments&#xD;&#xA;&#x9;For Each column As DataColumn In Centralizator.Columns.Cast(Of DataColumn).Skip(2)&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;Dim SortList As New List(Of Double)&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;SortList = (From r As DataRow In in_TransactionData.Rows.Cast(Of DataRow)()Select convert.ToDouble(r(column.ColumnName))).ToList&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;MED = Median(SortList)&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;MedList.Add(MED.ToString)&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#x9;Next&#xD;&#xA;&#xD;&#xA;&#x9;Centralizator.Rows.Add(MedList.ToArray) 'add medians to datatable&#xD;&#xA;&#xD;&#xA;Next&#xD;&#xA;&#xD;&#xA;End Sub&#xD;&#xA;&#xD;&#xA;'calculate median function&#xD;&#xA;Function Median(data As List(Of Double)) As Double&#xD;&#xA; data.Sort&#xD;&#xA;&#x9;If data.Count Mod 2 = 0 Then&#xD;&#xA;&#x9;&#x9;Return (data(data.Count \ 2 - 1) + data(data.Count \ 2)) / 2&#xD;&#xA;&#x9;Else&#xD;&#xA;&#x9;&#x9;Return data(data.Count \ 2)&#xD;&#xA;&#x9;End If&#xD;&#xA;End Function&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;Sub fakemain" DisplayName="Calculate Departament Medians"